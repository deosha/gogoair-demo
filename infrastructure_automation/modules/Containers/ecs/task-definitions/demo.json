[
  {
    "name": "demo-${env}",
    "image": "deojha1771/gogoair:${tag}",
    "memory": 256,
    "cpu": 128,
    "portMappings": [
      {
        "hostPort": 8000,
        "containerPort": 8000,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "demo-${env}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]